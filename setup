#!/bin/bash
go get github.com/restic/chunker
go get github.com/BurntSushi/toml
# github.com/vmihailenco/msgpack
go build dupver.go commit.go pack.go config.go randstring.go
echo "Done building"

install_folder=$HOME/.local/bin
mkdir -p $install_folder
cp dupver $install_folder
cp dupver-test.fish $HOME/Results
cp dupver-test.fish $HOME/Data/
echo "Copied to $install_folder"

